{
  "name": "@sutton-signwriting/sgnw-components",
  "description": "a javascript package of web components for use with the SignWriting script.",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "module-bundle": "dist/custom-elements-bundle/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/sgnw-components/sgnw-components.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.json": "stencil build --docs-json docs.json",
    "start": "stencil build --docs --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "wca": "wca analyze src",
    "wca.build": "wca analyze src --outFile custom-elements.json",
    "prestorybook": "npm run wca.build",
    "storybook": "start-storybook -s dist",
    "prestorybook.build": "npm run wca.build",
    "storybook.build": "build-storybook -s dist -o docs"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@stencil/core": "^1.17.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@types/jest": "^26.0.7",
    "@types/puppeteer": "2.0.1",
    "babel-loader": "^8.1.0",
    "puppeteer": "2.1.1",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "web-component-analyzer": "^1.1.6",
    "workbox-build": "4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@sutton-signwriting/core": "^1.2.0",
    "@sutton-signwriting/font-ttf": "^1.2.0"
  }
}
